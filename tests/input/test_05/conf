alphabet=DNA
alignment=false
input.sequence.file=../tests/input/test_05/minimal_aligned_simulated_nt.fa
input.sequence.sites_to_use=all
init.tree=distance
init.distance.method=infere_distance_matrix
model=PIP(model=GTR())
rate_distribution=Constant
optimization=ND-BFGS(derivatives=BFGS)
optimization.message_handler=../tests/input/test_05/test_05/opt_mh.log
optimization.profiler=../tests/input/test_05/opt_profiler.log
optimization.reparametrization=false
optimization.max_number_f_eval=10
optimization.tolerance=0.001
optimization.final=bfgs
optimization.topology=true
optimization.topology.algorithm=Swap(coverage="best-search",starting_nodes=Greedy(),max_cycles=50,tolerance=0.1,brlen_optimisation=Brent)
optimisation.alignment=false
output.msa.file=../tests/input/test_05/out_alignment.fa
